<%= table([{w: 3, t: '类型名称'}, {w: 3, t: '值的名称'}, {w: 3, t: '值'}, {w: 2, t: '操作'}],
          obj: list,
          pagi_param_name: 'page') do |l| %>
    <%= table_tr({c: seq(list, l, params[:page]||1)},
                 {c: l.type_name},
                 {c: l.value},
                 {c: l.code},
                 {c: action_links([{value: '编辑', url: edit_fdn_lookup_path(:id => l.id)}, {value: '删除', url: fdn_lookup_path(l), url_method: 'delete', confirm: '确定要删除吗？'}])}) %>
<% end %>
