<%= table(fdn_lookup_header,
          obj: obj,
          fixed: true,
          pagi_param_name: 'page') do |o| %>
    <%= table_tr({c: seq(obj, o, params[:page]||1)},
                 {c: o.type_name},
                 {c: o.value},
                 {c: o.code},
                 {c: o.seq},
                 {c: action_links([
                                          {value: '编辑', url: edit_fdn_lookup_path(:id => o.id)},
                                          {value: '删除', url: fdn_lookup_path(o), url_method: 'delete', confirm: '确定要删除吗？'}
                                  ]
                 )}
        ) %>
<% end %>