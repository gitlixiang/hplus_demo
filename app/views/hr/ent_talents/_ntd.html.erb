<div class="ibox-content">
<%= table(search_ent_talent_header,
          obj: talent,
          fixed: true,
          pagi_param_name: 'page') do |tal| %>
    <%= table_tr({c: seq(talent, tal, params[:page]||1)},
                 {c: tal.name},
                 {l: tal.ent_name},
                 {c: tal.age},
                 {c: tal.sex == 'F' ? '女' : '男'},
                 {c: tal.work_date},
                 {c: action_links([
                                          {value: '查看', url: hr_ent_talent_path(tal)},
                                          {value: '确认', url: check_hr_ent_talents_path(id:tal.id),url_method:'get'}
                                  ]
                 )}
        ) %>
<% end %>
</div>